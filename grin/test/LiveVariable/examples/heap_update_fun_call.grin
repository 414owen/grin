grinMain =
  n0 <- pure (CBool 0)
  n1 <- pure (CWord 0)
  p0 <- f 0 n0 n1
  n2 <- pure (CNode 0)
  q0 <- g p0 n2
  n3 <- fetch p0
  case n3 of
    (CBool c0) -> pure c0
    (CWord c1) -> pure 0
    (CNode c2) -> pure c2
    (CNope c3) -> pure c3

f x y z =
  case 0 of
    0 -> store y
    1 -> store z

g u v =
  update u v

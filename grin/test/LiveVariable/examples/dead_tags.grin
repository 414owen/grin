grinMain =
  n0 <- pure (CNil)
  p0 <- store n0
  n1 <- pure (CCons 0 p0)
  p1 <- store n1

  update p0 (CInt 5)
  update p1 (CInt 5)

  n4 <- case 0 of
    0 ->
      n2 <- fetch p0
      pure n2
    1 ->
      n3 <- fetch p1
      pure n3

  case n4 of
    (CWord c0) -> pure 0
    #default   -> pure 1

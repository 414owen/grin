grinMain =
  n0 <- f 0
  b0 <- pure 0
  b1 <- pure 0
  case n0 of
    (CBool c0) -> case n0 of
                    (CBool c1) -> pure (CBool c1)
                    (CWord c2) -> pure (CWord b0)
                    (CNope c3) -> pure (CNope c3)
    (CWord c4) -> case n0 of
                    (CBool c5) -> pure (CBool b1)
                    (CWord c6) -> pure (CWord c4)
                    (CNope c7) -> pure (CCNope c7)
f x =
  case x of
    0 -> pure (CBool 0)
    1 -> pure (CWord 0)

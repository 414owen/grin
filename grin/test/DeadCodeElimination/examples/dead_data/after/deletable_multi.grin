grinMain =
  a0 <- pure 0
  a1 <- pure 0
  a2 <- pure 0
  a3 <- pure 0
  a4 <- pure 0
  a5 <- pure 0

  -- two producers
  n0 <- pure (CThree.0 a1)
  n1 <- pure (CThree.0 a4)

  -- n01 has producers: n0, n1
  n01 <- case 0 of
    0 -> pure n0
    1 -> pure n1

  -- consumers
  (CThree.0 b1) <- pure n0
  b2 <- pure (#undefined :: T_Int64)
  b0 <- pure (#undefined :: T_Int64)

  (CThree.0 c1) <- pure n01
  c2 <- pure (#undefined :: T_Int64)
  c0 <- pure (#undefined :: T_Int64)

  (CThree.0 d1) <- pure n1
  d2 <- pure (#undefined :: T_Int64)
  d0 <- pure (#undefined :: T_Int64)

  r <- pure (CLive c1)
  pure r

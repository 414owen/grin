grinMain =
  a0 <- pure (CInt 5)
  a1 <- pure (CInt 5)
  a2 <- pure (CInt 5)
  p0 <- store a0
  p1 <- store a1
  p2 <- store a2

  foo3   <- pure (P3foo)

  (P3foo) <- pure foo3
  foo2    <- pure (P2foo p0)

  (P2foo v0) <- pure foo2
  foo1       <- pure (P1foo v0 p1)

  (P1foo v1 v2) <- pure foo1
  fooRet        <- foo v1 v2 p2

  pure fooRet


foo x0 y0 z0 =
  y0' <- fetch y0
  (CInt n) <- y0'
  pure y0'


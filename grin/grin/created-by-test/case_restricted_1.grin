grinMain =
  a0 <- f 0
  r <- case a0 of
    (CInt c0)  -> b0 <- pure (CInt 5)
                  pure b0
    (CBool c1) -> b1 <- pure (CBool 0)
                  pure b1
    (CNope c2) -> b2 <- pure (CNope 1)
                  pure b2
  pure r


f x =
 case x of
  0 -> x0 <- pure (CInt 5)
       pure x0
  1 -> x1 <- pure (CBool 0)
       pure x1

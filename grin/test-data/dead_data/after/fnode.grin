grinMain =
  x0 <- pure (CInt 5)
  p0 <- store x0
  a0 <- pure (Ffoo.0 p0)
  p1 <- store a0
  a1 <- eval p1
  pure a1

foo x y z =
  y' <- eval y
  pure y'

eval p =
  v <- fetch p
  case v of
    (CInt n) -> pure v
    (Ffoo.0 y1) ->
      z1 <- pure (#undefined :: #ptr)
      x1 <- pure (#undefined :: #ptr)
      w <- foo x1 y1 z1
      update p w
      pure w

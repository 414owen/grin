-- create value, place it on the heap
-- create thunk, return thunk

grinMain =
  (CInt k) <- eval id_one
  _prim_int_print k

id_one =
  one     <- pure (CInt 1)
  one_ptr <- store one
  thunk   <- pure (Fid one)
  pure thunk

eval p =
  v <- fetch p
  case v of
    (Fid x.1) ->
      r.id <- id x.1
      update p r.id
      pure r.id

id x.0 =
  x.0' <- eval x.0
  pure x.0'
